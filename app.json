      ],
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.minminzei.expodemo",
      "supportsTablet": true,
      "isTabletOnly": false,
      "icon": "./assets/icon.png",
      "buildNumber": "1",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "写真をアップロードできるようになります",
        "NSCameraUsageDescription": "写真をアップロードできるようになります",
