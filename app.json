    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "./assets/*"
    ],
    "description": "expoデモアプリ",
    "android": {
      "package": "com.minminzei.expodemo",
      "icon": "./assets/icon.png",
